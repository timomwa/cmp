/* CSS Document */
body {
	
	/*background-color: #F5D0A9;*/
	background:url(../images/bg.gif);
	font-size: 60%;
	text-align:center;
	
	margin:10px 0;
	padding:0;
	width: 100%;
	/*width: auto ;*/
}
/**{
	margin:2px;
	padding:0px;
	
}*/

body{
	background-color:#8dc711;
	font-family: Arial;
	font-size: 12px;
	color:#363636;
}

a{
	color:#363636;
	text-decoration:none;
}
a:hover{
	text-decoration:underline;
}

.curved{
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	 -khtml-border-radius:10px;
	  border-radius:10px;
}

/***************Login**************/
div#login_container{
	width:600px;
	height:auto;
	margin-left:auto;
	margin-right:auto;
}

div.login_logo{
	width:600px;
	height:283px;
	background:url(../images/senqlogo_.jpg) center no-repeat;
}

div.login_form{
	width:68%;
	margin-left:16%;
	margin-right:16%;
}

div.login_form .login_input{
	width:300px;
	height:20px;
	padding:1px;
}

div.login_form .login_btn{
	width:100px;
	height:20px;
	border:#8DC710 1px solid;
	background-color:#ffffff;
	color:#000000;
}

/***********Home styles*************/

div#home_container{
	width:900px;
	margin-left:auto;
	margin-right:auto;
	
}

div#header{
	width:100%;
	float:left;
	background:url(../images/senqlogo_.jpg) left no-repeat #FFF;
	height:232px;
}

div#header .home_text{
	float:right;
	width:auto;
	margin-top:195px;
	margin-right:5%;
}

div#menu{
	width:100%;
	float:left;
	background:url(../images/home_menu.png) repeat-x;
}

div#menu a{
	float:left;
	padding-left:15px;
	padding-right:15px;
	padding-top:10px;
	margin-right:10px;
	list-style:none;
	height:30px;
	text-decoration:none;
	color:#E6E9EA;
}

div#menu a:hover{
	background:url(../images/home_menu_h.png) repeat-x;
	text-decoration:none;
}

#phone_voucher_num{
	width: 153px; height: 25px; display:block;
}
div#content{
	width:96%;
	float:left;
	background-color:#ffffff;
	padding:2%;
}


div#footer{
	float:left;
	width:100%;
	text-align:right;
	background-color:#ffffff;
	border-top:#E6E9EA 1px solid;
	padding-top:10px;
}

div.inmobia_footer_logo{
	background:url(../images/inmobialogo.png) no-repeat right;
	width:130px;
	height:40px;
	float:right;
}

div#search{
	width:100%;
	float:left;
	margin-top:5px;
	background-color:#FFF;
	border-bottom:#E6E9EA 1px solid;
}

div#search .dates{
	width:200px;
	height:20px;
}

div#search .search_btn{
	width:100px;
	height:20px;
	border:#df985f 1px solid;
	background-color:#df985f;
	color:#000000;
}

table.imagetable {
	font-family: verdana,arial,sans-serif;
	font-size:11px;
	color:#333333;
	border-width: 1px;
	border-color: #999999;
	border-collapse: collapse;
	clear:both;
}
table.imagetable th {
	background:#b5cfd2 url('../images/cell-blue.jpg');
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #999999;
}
.looser {
	background:#dcddc0 url('../images/cell-grey.jpg');
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #999999;
}
.winner {
	background:#52D017 url('../images/cell-grey-winner.jpg') ;
	border-width: 1px  ;
	padding: 0px  ;
	border-style: solid  ;
	border-color: #999999  ;
}
.voucher_c{
	height: 50px;
}
.big_input{
	font-size: 48px; 
	width: 8em;
}
.search_btn {
	-moz-box-shadow:inset 0px 1px 0px 0px #ffffff;
	-webkit-box-shadow:inset 0px 1px 0px 0px #ffffff;
	box-shadow:inset 0px 1px 0px 0px #ffffff;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #31B404), color-stop(1, #9AFE2E) );
	background:-moz-linear-gradient( center top, #31B404 5%, #9AFE2E 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#31B404', endColorstr='#9AFE2E');
	background-color:#298A08;
	-moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:6px;
	border:1px solid #0B610B;
	display:inline-block;
	color:#088A08;
	font-family:Arial Black;
	font-size:14px;
	font-weight:bold;
	padding:6px 24px;
	text-decoration:none;
	text-shadow:1px 1px 0px #80FF00;
}.search_btn:hover {
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #9AFE2E), color-stop(1, #31B404) );
	background:-moz-linear-gradient( center top, #9AFE2E 5%, #31B404 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#9AFE2E', endColorstr='#31B404');
	background-color:#298A08;
}.search_btn:active {
	position:relative;
	top:1px;
}
